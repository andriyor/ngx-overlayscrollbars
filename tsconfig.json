{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-overlayscrollbars": [
        "dist/ngx-overlayscrollbars/ngx-overlayscrollbars",
        "dist/ngx-overlayscrollbars"
      ],
      "my-lib-void": [
        "dist/my-lib-void/my-lib-void",
        "dist/my-lib-void"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "overlayscrollbars": [
        "./node_modules/overlayscrollbars"
      ]
    }
  }
}
